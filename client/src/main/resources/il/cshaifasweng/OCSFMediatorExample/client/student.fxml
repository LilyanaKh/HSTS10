            <Image url="@primaryImage.jpg" />
         </image>
 -->         
      </ImageView>
  
	<children>
		<ImageView fx:id="welcomesign" fitHeight="372.0"
			fitWidth="480.0" pickOnBounds="true" preserveRatio="true" />
		<Text id="welcomesign" layoutX="6.0" layoutY="54.0"
			strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome"
			textAlignment="CENTER" wrappingWidth="467.3333435058594">
